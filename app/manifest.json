{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contentSettings",
    "storage"
  ],
  "web_accessible_resources": [
    "css/*",
    "js/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/vsearch/p*",
        "https://www.linkedin.com/m/search/results/index/*"
      ],
      "css": [
        "css/extension.css"
      ],
      "js": [
        "js/vendor/d3.v3.min.js",
        "js/vendor/lodash.core.js",
        "js/vendor/jquery.min.js",
        "scripts/other.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
